logging {
  level  = "info"
  format = "logfmt"
}

remotecfg {
  url            = "https://fleet-management-prod-017.grafana.net"
  id             = sys.env("GRAFANA_COLLECTOR_ID")
  poll_frequency = "60s"

  basic_auth {
    username = sys.env("GRAFANA_COLLECTOR_ID")
    password = sys.env("GRAFANA_FLEET_TOKEN")
  }
}
